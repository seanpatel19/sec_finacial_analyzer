# Copy this file to .env and fill in your actual values

# SEC API Configuration (REQUIRED)
SEC_USER_AGENT=your-name-your-email@domain.com
SEC_REQUEST_DELAY=0.1

# LLM Configuration
MODEL_NAME=llama2:13b
MODEL_TEMPERATURE=0.3
MAX_CONTEXT_LENGTH=4096

# Processing Settings
CHUNK_SIZE=2000
CHUNK_OVERLAP=200

# Paths (optional - will use defaults if not specified)
DATA_DIR=./data
OUTPUT_DIR=./data/summaries

# Logging
LOG_LEVEL=INFO